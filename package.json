{
  "name": "expo-next-tamagui-monorepo",
  "version": "0.3.1",
  "private": true,
  "workspaces": ["./apps/*", "./packages/*"],
  "scripts": {
    "android": "bun run --filter '*/expo' android",
    "check:expo": "bun run --filter '*/expo' check",
    "clean": "git clean -xdf",
    "doctor": "bun run --filter '*/expo' doctor",
    "ios": "bun run --filter '*/expo' ios",
    "lint": "bunx @biomejs/biome check --write .",
    "native": "bun run --filter '*/expo' start",
    "postinstall": "bun run --filter '*/ui' build",
    "watch": "bun run --filter '*/ui' watch",
    "web": "bun run --filter '*/ui' build && bun run --filter '*/next' dev",
    "web:extract": "DISABLE_EXTRACTION=false bun run --fitler '*/next' dev",
    "web:prod": "bun run --filter '*/ui' build && bun run --filter '*/next' build",
    "web:prod:serve": "bun run --filter '*/next' serve"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.73.7",
    "react-native-web": "^0.19.11"
  },
  "devDependencies": {
    "@babel/types": "^7.24.5",
    "@tamagui/constants": "latest",
    "@types/node": "^20.12.10",
    "typescript": "^5.4.5"
  }
}
