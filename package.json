{
  "name": "expo-next-tamagui-monorepo",
  "version": "0.3.1",
  "private": true,
  "workspaces": ["./apps/*", "./packages/*"],
  "scripts": {
    "android": "bun --filter './apps/expo' android",
    "check": "bun --filter './apps/expo' check",
    "clean": "git clean -xdf",
    "doctor": "bun --filter './apps/expo' doctor",
    "ios": "bun --filter './apps/expo' ios",
    "lint": "bunx @biomejs/biome check --write .",
    "native": "bun --filter './apps/expo' start",
    "postinstall": "bun --filter '*/ui' build",
    "watch": "bun --filter '*/ui' watch",
    "web": "bun --filter '*/ui' build && bun --filter './apps/next' dev",
    "web:extract": "DISABLE_EXTRACTION=false bun --fitler './apps/next' dev",
    "web:prod": "bun --filter '*/ui' build && bun --filter './apps/next' build",
    "web:prod:serve": "bun --filter './apps/next' serve"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-web": "^0.19.12"
  },
  "devDependencies": {
    "@babel/types": "^7.24.7",
    "@tamagui/constants": "latest",
    "@types/bun": "^1.1.3",
    "@types/react": "~18.2.79",
    "@types/react-native": "^0.73.0",
    "typescript": "~5.3.3"
  }
}
